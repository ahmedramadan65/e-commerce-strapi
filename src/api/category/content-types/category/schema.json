{
  "kind": "collectionType",
  "collectionName": "categories",
  "info": {
    "singularName": "category",
    "pluralName": "categories",
    "displayName": "Category"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "unique": true,
      "required": true,
      "maxLength": 100,
      "minLength": 3
    },
    "slug": {
      "type": "string",
      "required": true,
      "unique": true,
      "maxLength": 100,
      "minLength": 3
    },
    "product": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::product.product",
      "mappedBy": "category"
    }
  }
}
